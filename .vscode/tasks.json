{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc.exe build active file",
            "command": "C:\\TDM-GCC-64\\bin\\gcc.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "src\\*.cpp",
                "-o",
                "${fileDirname}\\${fileBasenameNoExtension}.exe"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "label": "compile with TDM-GCC",
            "type": "shell",
            "command": "g++",
            "args": [
              "-g",
              "${workspaceFolder}\\main.cpp",
              "${workspaceFolder}\\src\\nfa.cpp",
              "${workspaceFolder}\\src\\dfa.cpp",
              "${workspaceFolder}\\src\\infix_to_postfix.cpp",
              "${workspaceFolder}\\src\\input_parser.cpp",
              "${workspaceFolder}\\src\\nfa_generator.cpp",
              "${workspaceFolder}\\src\\operations_handler.cpp",
              "${workspaceFolder}\\src\\state.cpp",
              "${workspaceFolder}\\src\\token.cpp",
              "${workspaceFolder}\\src\\utils.cpp",
              "${workspaceFolder}\\src\\globals.cpp",
              "${workspaceFolder}\\src\\SubsetConstruction.cpp",
              "${workspaceFolder}\\src\\lexical_analyzer.cpp",
              "${workspaceFolder}\\src\\syntax_parser.cpp",
              "${workspaceFolder}\\src\\first_follow_builder.cpp",
              "${workspaceFolder}\\src\\ll1_processor.cpp",
              "${workspaceFolder}\\src\\LL1Parser.cpp",
              "${workspaceFolder}\\src\\parse_tree_builder.cpp",
              "-o",
              "${workspaceFolder}\\main.exe",
              "-I.\\src"
            ],
            "group": {
              "kind": "build",
              "isDefault": true
            }
          }
    ],
    "version": "2.0.0"
}